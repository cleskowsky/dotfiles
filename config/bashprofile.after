# vim
export EDITOR=vim
export VISUAL=vim

# GOPATH
GOPATH=$HOME
export GOPATH

# paths
PATH=$HOME/bin:$PATH
PATH=$GOPATH/bin:$PATH
PATH=$HOME/workspace/dev_scripts/docker/bin:$PATH
PATH=$PATH:/usr/local/opt/go/libexec/bin
export PATH

# rbenv
eval "$(rbenv init -)"

# vagrant
alias vs="vagrant status"
alias vu="vagrant up"
alias vh="vagrant halt"
alias vd="vagrant destroy"

